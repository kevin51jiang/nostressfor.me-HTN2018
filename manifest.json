{
    "name": "No Stress For Me",
    "version": "0.1",
    "description": "Keep yourself postive and motivated throughtout the day",
    "manifest_version": 2,
    "permissions": ["storage"],
    "page_action": {
        "default_popup": "src/popup.html"
    },
    "background": {
        "scripts": ["src/background.js"],
        "persistent": true
    }
}